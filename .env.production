# Variables de entorno para PRODUCCIÓN (Vercel)
# Reemplaza estos valores con tus datos reales en el dashboard de Vercel

# Tipo de Base de Datos
DB_TYPE=postgresql

# Configuración de PostgreSQL (recomendado para producción)
PG_HOST=tu-host-postgres.com
PG_PORT=5432
PG_DATABASE=miparqueo
PG_USER=usuario_postgres
PG_PASSWORD=tu-contraseña-segura

# Configuración del Servidor
PORT=3000
NODE_ENV=production

# URL de la API (actualiza con tu URL de Vercel)
REACT_APP_API_URL=https://miparqueo-app.vercel.app
