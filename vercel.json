{
  "version": 2,
  "buildCommand": "npm install",
  "devCommand": "node server.js",
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
